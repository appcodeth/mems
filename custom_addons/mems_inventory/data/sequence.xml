<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_receive01" model="ir.sequence">
            <field name="name">Receive No</field>
            <field name="code">mems.receive_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">RCV%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_issue01" model="ir.sequence">
            <field name="name">Issue No</field>
            <field name="code">mems.issue_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">ISU%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_return01" model="ir.sequence">
            <field name="name">Return No</field>
            <field name="code">mems.return_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">RTN%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
