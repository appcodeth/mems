<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_pm01" model="ir.sequence">
            <field name="name">PM No</field>
            <field name="code">mems.pm_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">PM%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_calibrate01" model="ir.sequence">
            <field name="name">Calibrate No</field>
            <field name="code">mems.calibrate_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">CA%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
