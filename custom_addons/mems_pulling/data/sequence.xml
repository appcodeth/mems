<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_borrow01" model="ir.sequence">
            <field name="name">Borrow No</field>
            <field name="code">mems.borrow_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">BR%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_restore01" model="ir.sequence">
            <field name="name">Restore No</field>
            <field name="code">mems.restore_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">RS%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
