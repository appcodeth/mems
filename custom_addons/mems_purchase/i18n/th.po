#. module: mems_purchase
#: model:mail.template,body_html:mems_purchase.mail_template_purchase_form
msgid "<?xml version=\"1.0\"?>\n"
"<data><p>เรียน ${object.supplier_id.name}</p>\n"
"                <p>\n"
"                    Here is your purchase\n"
"                    <strong>${object.name}</strong>\n"
"                </p>\n"
"                <p>You can reply to this email if you have any questions.</p>\n"
"                <p>Thank you</p>\n"
"            </data>"
msgstr "<?xml version=\"1.0\"?>\n"
"<data><p>เรียน ${object.supplier_id.name}</p>\n"
"                <p>\n"
"                    เราส่งใบสั่งซื้อสินค้า ตามไฟล์แนบ\n"
"                    <strong>${object.name}</strong>\n"
"                </p>\n"
"                <p>สอบถามข้อมูลเพิ่มเติม สามารถตอบกลับได้ที่อีเมลนี้</p>\n"
"                <p>ขอบคุณ</p>\n"
"            </data>"

#. module: mems_purchase
#: model:mail.template,body_html:mems_purchase.mail_template_rfq_form
msgid "<?xml version=\"1.0\"?>\n"
"<data><p>Dear ${object.supplier_id.name}</p>\n"
"                <p>\n"
"                    Here is your quotation\n"
"                    <strong>${object.name}</strong>\n"
"                </p>\n"
"                <p>You can reply to this email if you have any questions.</p>\n"
"                <p>Thank you</p>\n"
"            </data>"
msgstr "<?xml version=\"1.0\"?>\n"
"<data><p>เรียน ${object.supplier_id.name}</p>\n"
"                <p>\n"
"                    เราส่งใบขอราคาสินค้า ตามไฟล์แนบ\n"
"                    <strong>${object.name}</strong>\n"
"                </p>\n"
"                <p>สามารถตอบกลับ และส่งราคาสินค้ามาได้ที่อีเมลนี้</p>\n"
"                <p>ขอบคุณ</p>\n"
"            </data>"

