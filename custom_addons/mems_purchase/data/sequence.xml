<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_rfq01" model="ir.sequence">
            <field name="name">RFQ No</field>
            <field name="code">mems.rfq_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">RFQ%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_purchase01" model="ir.sequence">
            <field name="name">Purchase Order No</field>
            <field name="code">mems.purchase_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">PO%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="seq_payment03" model="ir.sequence">
            <field name="name">Payment No</field>
            <field name="code">payment.payment_no</field>
            <field name="active">TRUE</field>
            <field name="prefix">PY%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
